{
  "name": "whats-in-store",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "cloudinary": "^1.14.0",
    "concurrently": "^4.1.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-form-data": "^2.0.8",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "mdbreact": "^4.12.0",
    "mongoose": "^5.4.20",
    "mongoose-bcrypt": "^1.6.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react-moment": "^0.8.4"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "node_modules/*"
    ]
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakescheele/whats-in-store.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jakescheele/whats-in-store/issues"
  },
  "homepage": "https://github.com/jakescheele/whats-in-store#readme"
}
